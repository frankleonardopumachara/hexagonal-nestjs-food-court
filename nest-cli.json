{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/food-court/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/food-court/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/food-court",
  "projects": {
    "food-court": {
      "type": "application",
      "root": "apps/food-court",
      "entryFile": "main",
      "sourceRoot": "apps/food-court/src",
      "compilerOptions": {
        "tsConfigPath": "apps/food-court/tsconfig.app.json"
      }
    },
    "my-app": {
      "type": "application",
      "root": "apps/my-app",
      "entryFile": "main",
      "sourceRoot": "apps/my-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my-app/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "courts": {
      "type": "application",
      "root": "apps/courts",
      "entryFile": "main",
      "sourceRoot": "apps/courts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/courts/tsconfig.app.json"
      }
    },
    "users-microservice": {
      "type": "application",
      "root": "apps/users-microservice",
      "entryFile": "main",
      "sourceRoot": "apps/users-microservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users-microservice/tsconfig.app.json"
      }
    },
    "food-court-microservice": {
      "type": "application",
      "root": "apps/food-court-microservice",
      "entryFile": "main",
      "sourceRoot": "apps/food-court-microservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/food-court-microservice/tsconfig.app.json"
      }
    },
    "traceability-microservice": {
      "type": "application",
      "root": "apps/traceability-microservice",
      "entryFile": "main",
      "sourceRoot": "apps/traceability-microservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/traceability-microservice/tsconfig.app.json"
      }
    },
    "messaging-microservice": {
      "type": "application",
      "root": "apps/messaging-microservice",
      "entryFile": "main",
      "sourceRoot": "apps/messaging-microservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/messaging-microservice/tsconfig.app.json"
      }
    }
  }
}